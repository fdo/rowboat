select * from todo_running where rowid > 1010;
-- display last bit of table

update todo_running set timeenter = '2022-05-12 09:05:05' where rowid = 1018 ;
update todo_running set timeenter = '2022-05-14 09:05:05' where rowid = 1019 ;
update todo_running set timeenter = '2022-05-16 09:05:05' where rowid = 1020 ;
select * from todo_running where rowid > 1010;

